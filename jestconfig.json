{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["js", "mjs", "cjs", "jsx", "ts", "tsx", "json", "node"],
  "testEnvironment": "node",
  "setupFiles": ["<rootDir>/jest-shim.js"],
  "clearMocks": true,
  "collectCoverage": true,
  "coverageProvider": "v8",
  "coverageDirectory": "coverage"
}